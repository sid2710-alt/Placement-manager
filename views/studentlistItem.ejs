<div class="studentCard">
    <div>
      <span class="cardHead"><strong><%= student.name %></strong></span>
    </div>
    <div>
      <span>
        <strong
          >Batch: <%= student.batch %> | Status: <%= student.status%></strong
        ></span
      >
    </div>
    <div class="scoreHead"><strong>Scores</strong></div>
    <div class="score">
      <div class="scoreSub">
        <div>DSA</div>
        <div><%= student.DSA%></div>
      </div>
      <div class="scoreSub">
        <div>MERN</div>
        <div><%= student.MERN%></div>
      </div>
      <div class="scoreSub">
        <div>APP_DEV</div>
        <div><%= student.APP_DEV%></div>
      </div>
    </div>
  
    <div class="studentCardActions">
      <div class="cardLinks">
        <a href="/student/show-student-details/<%= student.id %>">View Details</a>
      </div>
      <div style="margin-left: 10px" class="cardLinks">
        <a href="/student/edit-student/<%= student.id %>">Edit Details</a>
      </div>
    </div>
  </div>